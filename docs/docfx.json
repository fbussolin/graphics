{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
	  "msdos"
    ],
    "globalMetadata": {
      "_appName": "graphics",
      "_appTitle": "graphics",
	  "_disableBreadcrumb": true,
	  "_disableContribution": true,
	  "_disableNextArticle": true,
      "_enableSearch": true,
      "pdf": true
    }
  }
}